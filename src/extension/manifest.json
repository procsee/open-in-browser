{
    "name": "Open in browser",
    "description": "Opens the selected link in another browser.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions":
    [
        "contextMenus",
        "storage"
    ],
    "background":
    {
        "service_worker": "background/background.js",
        "type": "module"
    },
    "action":
    {
        "default_popup": "popup/popup.html"
    },
    "icons":
    {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "64": "icons/icon64.png",
        "128": "icons/icon128.png",
        "256": "icons/icon256.png",
        "512": "icons/icon512.png"
    }
}